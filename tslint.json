{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "no-namespace": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": false,
    "radix": false,
    "no-consecutive-blank-lines": false,
    "no-string-throw": false,
    "no-string-literal": false,
    "align": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "class-name": false,
    "variable-name": false,
    "member-access": false,
    "member-ordering": false,
    "max-classes-per-file": false,
    "array-type": false,
    "no-empty": false,
    "prefer-for-of": false
  },
  "rulesDirectory": []
}